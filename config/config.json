{
  "development": {
    "username": "${DB_USERNAME}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_NAME}",
    "host": "${DB_HOST}",
    "port": "5432",
    "dialect": "postgres",
    "logging": true
  },
  "test": {
    "username": "${DB_USERNAME}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_NAME_TEST}",
    "host": "${DB_HOST}",
    "port": "5432",
    "dialect": "postgres",
    "logging": false
  },
  "production": {
    "username": "${DB_USERNAME}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_NAME_PROD}",
    "host": "${DB_HOST}",
    "port": "5432",
    "dialect": "postgres",
    "logging": false,
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}